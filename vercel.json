{
  "version": 2,
  "builds": [
    {
      "src": "api/player.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/player",
      "dest": "/api/player"
    },
    {
      "src": "/player/(.*)",
      "dest": "/api/player"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
