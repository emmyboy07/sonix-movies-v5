{
  "version": 2,
  "builds": [
    {
      "src": "api/player.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/proxy-sub",
      "dest": "/api/player.js"
    },
    {
      "src": "/all/player/(.*)",
      "dest": "/api/player.js"
    },
    {
      "src": "/movie/(\\d+)",
      "dest": "/api/player.js"
    },
    {
      "src": "/tv/(\\d+)/(\\d+)/(\\d+)",
      "dest": "/api/player.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/player.js"
    }
  ]
}
